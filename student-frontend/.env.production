# Production environment variables for Bunny CDN
# These are loaded when building for production (npm run build)

# Backend API base URL for production
VITE_API_BASE_URL=https://student.skonzify.com/api/v1

# Application name for production
VITE_APP_NAME=LMS-Student

# Production settings - disable debugging and logging
VITE_ENABLE_LOGGING=false
VITE_USE_MOCKS=false
VITE_ENABLE_DEBUG_PANEL=false
VITE_ENABLE_PERFORMANCE_MONITORING=false

# Production CDN settings - TEMPORARILY DISABLED until images are uploaded
VITE_BUNNY_CDN_ENABLED=false
VITE_BUNNY_CDN_BASE_URL=https://lmsbunny.b-cdn.net

# Image optimization settings
VITE_DEFAULT_IMAGE_QUALITY=85
VITE_DEFAULT_IMAGE_FORMAT=webp
VITE_ENABLE_IMAGE_PRELOAD=true
VITE_LAZY_LOADING_ENABLED=true

# Security settings
VITE_TOKEN_ENCRYPTION_ENABLED=true
VITE_SESSION_TIMEOUT=3600000