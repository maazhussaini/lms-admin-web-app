# Development environment variables
# These are loaded when running in development mode (npm run dev)

# Backend API base URL for development
VITE_API_BASE_URL=http://localhost:3000/api/v1

# Development-specific settings
VITE_ENABLE_LOGGING=true
VITE_USE_MOCKS=false

# Debug and development tools
VITE_ENABLE_DEBUG_PANEL=true
VITE_ENABLE_PERFORMANCE_MONITORING=true

# Development CDN settings (disabled for local development)
VITE_BUNNY_CDN_ENABLED=true
VITE_BUNNY_CDN_BASE_URL=https://lmsbunny.b-cdn.net
