{
  "name": "backend",
  "version": "1.0.0",
  "main": "dist/backend/src/server.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "build:watch": "tsc --project tsconfig.build.json --watch",
    "build:clean": "npm run clean && npm run build",
    "clean": "rimraf dist",
    "dev": "tsx watch --clear-screen=false src/server.ts",
    "dev:debug": "tsx watch --inspect --clear-screen=false src/server.ts",
    "start": "node dist/server.js",
    "start:prod": "cross-env NODE_ENV=production node dist/server.js",
    "type-check": "tsc --project tsconfig.development.json",
    "type-check:watch": "tsc --project tsconfig.development.json --watch",
    "type-check:build": "tsc --project tsconfig.build.json --noEmit",
    "type-check:strict": "tsc --noEmit --strict",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "lint:check": "eslint src --ext .ts --max-warnings 0",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern=unit",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:prod": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:seed": "tsx prisma/seed.ts",
    "prisma:reset": "prisma migrate reset",
    "prisma:generate:schema": "node scripts/generate-prisma-schema.js",
    "prisma:generate:dev": "npm run prisma:generate:schema && prisma generate",
    "prisma:migrate:dev": "npm run prisma:generate:schema && prisma migrate dev",
    "setup": "npm install && npm run prisma:generate:dev && npm run prisma:migrate",
    "setup:fresh": "npm run clean && npm install && npm run prisma:reset && npm run prisma:seed",
    "dev:full": "npm run type-check && npm run lint:check && npm run test:unit && npm run dev",
    "dev:validate": "npm run type-check:build && npm run lint:check",
    "prebuild": "npm run type-check:build && npm run lint:check",
    "postbuild": "echo 'Build completed successfully'",
    "deploy:check": "npm run build && npm run test:ci && npm run prisma:migrate:prod",
    "deploy:validate": "npm run type-check:build && npm run lint:check && npm run test:ci",
    "deps:check": "npm outdated",
    "deps:update": "npm update",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "dev:type-watch": "concurrently \"npm run type-check:watch\" \"npm run dev\"",
    "clean:all": "npm run clean && rimraf node_modules && rimraf .tsbuildinfo"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.8.2",
    "bcryptjs": "^3.0.2",
    "body-parser": "^2.2.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.2.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "ms": "^2.1.3",
    "multer": "^2.0.0",
    "rimraf": "^6.0.1",
    "socket.io": "^4.7.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "tslib": "^2.8.1",
    "winston": "^3.12.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/body-parser": "^1.19.5",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^22.15.23",
    "@types/socket.io": "^3.0.1",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.31.0",
    "jest": "^29.7.0",
    "prisma": "^6.8.2",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
